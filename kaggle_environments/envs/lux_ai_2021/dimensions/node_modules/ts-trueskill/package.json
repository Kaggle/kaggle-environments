{
  "name": "ts-trueskill",
  "version": "2.5.5",
  "description": "Port of python trueskill package in TypeScript",
  "repository": "scttcper/ts-trueskill",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "main": "./dist/index.js",
  "module": "./dist/module/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "benchmark": "ts-node -P ./benchmark/tsconfig.json benchmark/index.ts",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@types/mathjs": "6.0.4",
    "lodash": "^4.17.15",
    "mathjs": "^6.6.1",
    "ts-gaussian": "^1.2.3",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/fs-extra": "8.1.0",
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.149",
    "@types/node": "13.9.0",
    "@types/uuid": "7.0.0",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "benchmark": "2.1.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-xo-space": "0.24.0",
    "eslint-config-xo-typescript": "0.26.0",
    "eslint-plugin-import": "2.20.1",
    "jest": "25.1.0",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.4",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typedoc": "0.16.11",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "keywords": [
    "typescript",
    "trueskill",
    "ranking",
    "player skill"
  ]
}

{
  "name": "dimensions-ai",
  "version": "4.7.1",
  "description": "A generalized AI Competition framework that allows you to create any competition you want in any language you want with no hassle.",
  "main": "./lib/main/index.js",
  "types": "./lib/main/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "test": "./runtests.sh",
    "test:storage": "nyc --reporter=html --no-clean mocha tests/core/storage/**/*.spec.ts",
    "test-seed": "node tests/seed/seedmongo.js",
    "report": "nyc report",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov",
    "docs": "typedoc --gitRevision master --theme ./theme/dimensions/ --mode modules --exclude ./src/Station/ --out docs ./src && touch ./docs/.nojekyll ",
    "watch": "tsc -w",
    "build": "rm -rf lib && tsc -p tsconfig.json && tsc -p tsconfiges6.json",
    "clean": "./cleanup.sh",
    "services:start": "docker-compose -f tests/dimension-docker/docker-compose.yml up -d",
    "services:stop": "docker-compose -f tests/dimension-docker/docker-compose.yml down",
    "prettier": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts"
  },
  "keywords": [
    "AI",
    "competition",
    "framework"
  ],
  "author": "StoneT2000",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/datastore": "^6.1.0",
    "@google-cloud/storage": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dockerode": "^3.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "extract-zip": "^2.0.0",
    "firebase-admin": "^9.2.0",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.11",
    "nanoid": "^3.1.4",
    "pidusage": "^2.0.18",
    "seedrandom": "^3.0.5",
    "sprintf-js": "^1.1.2",
    "tree-kill": "^1.2.2",
    "ts-trueskill": "^2.5.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-subset": "^1.3.3",
    "@types/dockerode": "^2.5.34",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/formidable": "^1.0.31",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.7.14",
    "@types/node": "^12.19.3",
    "@types/pidusage": "^2.0.1",
    "@types/seedrandom": "^2.4.28",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-subset": "^1.6.0",
    "eslint": "^7.7.0",
    "mocha": "^7.1.1",
    "mongo-seeding": "^3.4.1",
    "nyc": "^15.0.1",
    "prettier": "2.0.5",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.7",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/StoneT2000/Dimensions.git"
  },
  "mocha": ".mocharc.json",
  "sideEffects": false
}
